{
  "name": "pro-match",
  "version": "1.0.0",
  "description": "",
  "author": "Laoma Nogueira",
  "private": true,
  "license": "MIT",
  "scripts": {
    "build": "tsc",
    "start": "node dist/main/server.js",
    "start:dev": "ts-node-dev -r tsconfig-paths/register --no-notify --inspect --transpile-only --ignore-watch node_modules --respawn src/main/server.ts",
    "lint": "eslint \"{src,tests}/**/*.ts\"",
    "lint:fix": "eslint \"{src,tests}/**/*.ts\" --fix",
    "format": "prettier --write \"src/**/*.ts\" \"tests/**/*.ts\"",
    "test": "jest --runInBand --detectOpenHandles --forceExit -c ./tests/jest.config.ts",
    "test:integ": "jest -c ./tests/jest-integration.ts",
    "test:cov": "jest --coverage -c ./tests/jest.config.ts",
    "test:unit": "jest -c ./tests/jest-unit.ts",
    "test:watch": "jest -c ./tests/jest-unit.ts --watchAll",
    "test:clearcache": "jest --clearCache"
  },
  "dependencies": {
    "axios": "^1.6.7",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.18.2",
    "http-status": "^1.7.4",
    "reflect-metadata": "^0.2.1"
  },
  "devDependencies": {
    "@faker-js/faker": "^8.4.1",
    "@golevelup/ts-jest": "^0.4.0",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.11.19",
    "@types/supertest": "^6.0.2",
    "@typescript-eslint/eslint-plugin": "^7.0.2",
    "eslint": "^8.56.0",
    "jest": "^29.7.0",
    "prettier": "3.2.5",
    "supertest": "^6.3.4",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.3.3"
  }
}
